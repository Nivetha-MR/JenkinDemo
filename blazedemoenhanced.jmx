    String price   = vars.get(&quot;dc_price_&quot;    + i + &quot;_g1&quot;)

    int digits = flight.length()

    if (digits &gt; maxDigits) {
        maxDigits = digits
        maxFlights.clear()
        maxFlights.add([
            flight  : flight,
            airline : airline,
            price   : price
        ])
    }
    else if (digits == maxDigits) {
        maxFlights.add([
            flight  : flight,
            airline : airline,
            price   : price
        ])
    }
}
def selected = maxFlights[0]
vars.put(&quot;dc_selected_flight&quot;,  selected.flight)
vars.put(&quot;dc_selected_airline&quot;, selected.airline)
vars.put(&quot;dc_selected_price&quot;,   selected.price)
vars.put(&quot;dc_sel_digits&quot;,  maxDigits.toString())

log.info(&quot;===== FLIGHT WITH HIGHEST DIGITS =====&quot;)
