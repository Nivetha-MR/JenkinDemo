int total = vars.get(&quot;dc_sorted_count&quot;) as int

int pick = ((iteration - 1) % total) + 1

vars.put(&quot;dc_selected_airline&quot;, vars.get(&quot;dc_sorted_airline_&quot; + pick))
vars.put(&quot;dc_selected_flight&quot;,  vars.get(&quot;dc_sorted_flight_&quot; + pick))
