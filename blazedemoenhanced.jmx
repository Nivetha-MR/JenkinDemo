              <stringProp name="cacheKey">true</stringProp>
              <stringProp name="script">int count = (vars.get(&apos;dc_airline_matchNr&apos;) ?: &apos;0&apos;) as int

if (count == 0) {
    log.error(&quot;No airline data found&quot;)
    return
}
def rows = []
for (int i = 1; i &lt;= count; i++) {
    rows.add([
        index   : i,
        airline : vars.get(&quot;dc_airline_&quot; + i + &quot;_g1&quot;),
        flight  : vars.get(&quot;dc_flight_&quot; + i + &quot;_g1&quot;),
        price   : vars.get(&quot;dc_price_&quot; + i + &quot;_g1&quot;)
    ])
}
rows.sort { a, b -&gt;
    a.airline &lt;=&gt; b.airline
}

for (int i = 0; i &lt; rows.size(); i++) {
    vars.put(&quot;dc_sorted_airline_&quot; + (i + 1), rows[i].airline)
    vars.put(&quot;dc_sorted_flight_&quot;  + (i + 1), rows[i].flight)
    vars.put(&quot;dc_sorted_price_&quot;   + (i + 1), rows[i].price)
    vars.put(&quot;dc_sorted_index_&quot;   + (i + 1), rows[i].index.toString()) 
}

vars.put(&quot;dc_sorted_count&quot;, rows.size().toString())
log.info(&quot;Sorted Airlines (ASC): &quot; + rows*.airline)
