String price    = vars.get(&quot;dc_price_&quot; + minIndex)

vars.put(&quot;dc_selected_flight&quot;, flightNo)
vars.put(&quot;dc_selected_airline&quot;, airline)
vars.put(&quot;dc_selected_price&quot;, price)

log.info(&quot;Shortest Flight Details:&quot;)
