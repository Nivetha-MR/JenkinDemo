              <stringProp name="script">import java.time.*
import java.time.format.*
int count = (vars.get(&apos;dc_time_matchNr&apos;) ?: &apos;0&apos;) as int
if (count == 0) {
    vars.put(&apos;dc_min_duration&apos;, &apos;NOT_FOUND&apos;)
    vars.put(&apos;dc_min_duration_index&apos;, &apos;NOT_FOUND&apos;)
    return
}
DateTimeFormatter fmt = DateTimeFormatter.ofPattern(&quot;h:mm a&quot;, Locale.ENGLISH)
long minMinutes = Long.MAX_VALUE
int minIndex = -1
for (int i = 1; i &lt;= count; i++) {
String departStr = vars.get(&quot;dc_time_&quot; + i + &quot;_g1&quot;)
String arriveStr = vars.get(&quot;dc_time_&quot; + i + &quot;_g2&quot;)

    LocalTime depart = LocalTime.parse(departStr, fmt)
    LocalTime arrive = LocalTime.parse(arriveStr, fmt)

    long minutes = Duration.between(depart, arrive).toMinutes()
    if (minutes &lt; 0) {
        minutes += 24 * 60
    }
    log.info(&quot;Flight ${i} → ${departStr} to ${arriveStr} = ${minutes} mins&quot;)
    if (minutes &lt; minMinutes) {
        minMinutes = minutes
        minIndex = i
    }
}
vars.put(&quot;dc_min_duration&quot;, minMinutes.toString())
vars.put(&quot;dc_min_duration_index&quot;, minIndex.toString())
